package com.Collectio;

import java.util.*;

public class RemoveDuplicateByForEachLoop {

	public static void main(String[] args) {
		List<Integer> list=new ArrayList<>();
		list.add(1);
		list.add(2);
		list.add(3);
		list.add(1);
		list.add(2);
		list.add(3);
		list.add(1);
		list.add(3);
		System.out.println(list);
		ArrayList<Integer> uniqueElements=new ArrayList<>();
		for(Integer removeduplicte:list)
		{
			if(!uniqueElements.contains(removeduplicte))
				uniqueElements.add(removeduplicte);
		}
		System.out.println("duplicate"+list);
		System.out.println("uniqueElements"+uniqueElements);

	}

}
