package com.java;

public class ArmstrongNumber {

	public static void main(String[] args) {
		int no=153;
		int rem=0;
		
		int temp=0;
		while(no>0)
		{
			rem=no%10;
			temp=temp+(rem*rem*rem);
			no=no/10;
		}
		if(temp==no)
		{
			System.out.println("no is armstrong number");
		}
		else {
			System.out.println("not a Armstrong number");
		}
	}

}
