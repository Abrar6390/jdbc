package com.Collectio;

import java.util.*;

public class MapIterator {

	public static void main(String[] args) {
		Map<Integer,String>map=new HashMap<>();
		map.put(1,"Abrar");
		map.put(2,"Khan");
		map.put(3,"Shadab");
		map.put(4,"Khan");
		System.out.println(map);
       for (Map.Entry<Integer,String> i : map.entrySet()) {
		System.out.println(i);
	}
	}

}
